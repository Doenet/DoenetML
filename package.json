        "rimraf": "^6.1.2",
        "rollup-plugin-polyfill-node": "^0.13.0",
        "rollup-plugin-visualizer": "^6.0.5",
        "seedrandom": "^3.0.5",
        "source-map-generator": "^2.0.2",
        "tailwindcss": "^3.4.19",
