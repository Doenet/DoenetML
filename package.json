{
    "name": "doenetml",
    "version": "1.0.0",
    "private": true,
    "description": "Monorepo for the DoenetML project",
    "homepage": "https://github.com/Doenet/DoenetML/README.md",
    "bugs": {
        "url": "https://github.com/Doenet/DoenetML/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Doenet/DoenetML.git"
    },
    "license": "ISC",
    "author": "",
    "workspaces": [
        "./packages/ui-components",
        "./packages/utils",
        "./packages/static-assets",
        "./packages/parser",
        "./packages/lsp-tools",
        "./packages/lsp",
        "./packages/doenetml-worker-javascript",
        "./packages/debug-hooks",
        "./packages/doenetml-worker-rust/lib-js-wasm-binding/pkg",
        "./packages/doenetml-worker",
        "./packages/virtual-keyboard",
        "./packages/codemirror",
        "./packages/doenetml",
        "./packages/doenetml-prototype",
        "./packages/docs-nextra",
        "./packages/*"
    ],
    "scripts": {
        "build": "npm run build --workspace packages/doenetml",
        "build:all-no-docs": "npm run build -w packages/doenetml -w packages/doenetml-prototype -w packages/standalone -w packages/doenetml-iframe -w packages/v06-to-v07 -w packages/vscode-extension -w packages/test-viewer -w packages/test-cypress",
        "build:docs": "npm run build --workspace packages/docs-nextra",
        "build:all": "npm run build -ws --if-present",
        "clean": "rimraf packages/*/dist",
        "dev": "npm run dev --workspace packages/doenetml",
        "docs": "npm run build --workspace packages/docs-nextra & npm run dev --workspace packages/docs-nextra",
        "prettier:check": "prettier --check .",
        "prettier:format": "prettier --write .",
        "test": "npm run test -ws --if-present -- run"
    },
    "prettier": {
        "tabWidth": 4
    },
    "overrides": {
        "@qualified/lsp-connection": {
            "vscode-jsonrpc": "$vscode-jsonrpc"
        },
        "@qualified/vscode-jsonrpc-ww": {
            "vscode-jsonrpc": "$vscode-jsonrpc"
        },
        "vscode-languageserver-protocol": {
            "vscode-jsonrpc": "$vscode-jsonrpc"
        },
        "next": {
            "react": "$react",
            "react-dom": "$react-dom"
        },
        "react-copy-to-clipboard": {
            "react": "$react"
        },
        "@theguild/remark-mermaid": {
            "react": "$react"
        }
    },
    "devDependencies": {
        "@ariakit/react": "^0.4.20",
        "@codemirror/commands": "^6.10.0",
        "@codemirror/lang-xml": "^6.1.0",
        "@codemirror/language": "^6.11.3",
        "@codemirror/state": "^6.5.2",
        "@codemirror/view": "^6.38.8",
        "@emotion/react": "^11.14.0",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-regular-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@handsontable/react": "^12.4.0",
        "@lezer/common": "^1.4.0",
        "@lezer/generator": "^1.8.0",
        "@lezer/highlight": "^1.2.3",
        "@lezer/lr": "^1.4.5",
        "@lezer/xml": "^1.0.6",
        "@qualified/lsp-connection": "^0.3.0",
        "@qualified/vscode-jsonrpc-ww": "^0.3.0",
        "@reduxjs/toolkit": "^2.11.1",
        "@rollup/plugin-dsv": "^3.0.5",
        "@rollup/pluginutils": "^5.3.0",
        "@types/esprima": "^4.0.6",
        "@types/file-saver": "^2.0.7",
        "@types/node": "^25.0.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/react-measure": "^2.0.12",
        "@types/react-reconciler": "^0.32.3",
        "@types/seedrandom": "^3.0.8",
