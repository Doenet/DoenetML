        "@uiw/react-codemirror": "^4.25.3",
        "@vitejs/plugin-react": "^5.1.1",
        "@vitest/browser": "^4.0.15",
        "@vitest/web-worker": "^4.0.15",
        "@vscode/test-web": "^0.0.77",
        "autoprefixer": "^10.4.22",
        "axe-core": "^4.11.0",
        "better-react-mathjax": "^2.4.0-beta-1",
        "bootstrap": "^5.3.8",
        "chalk": "^5.6.2",
