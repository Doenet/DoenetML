{
    "name": "@doenet/static-assets",
    "type": "module",
    "description": "Static JSON blobs for DoenetML components",
    "version": "1.0.0",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": false,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./*json": {
            "import": "./dist/generated/*json"
        }
    },
    "scripts": {
        "test": "echo \"No tests \"",
        "build": "vite build",
        "build:schema": "vite-node ./scripts/generate-schema.ts && vite-node ./scripts/generate-entity-map.ts"
    }
}
