{
    "name": "@doenet/doenetml-worker-javascript",
    "type": "module",
    "description": "Repo for compiled WebWorker code for DoenetML",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": false,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./index.js": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "watch": "vite build --watch",
        "test": "vitest",
        "build": "wireit"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "src/**/*.js",
                "tsconfig.json",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../utils:build",
                "../parser:build"
            ]
        }
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "styled-components": "^5.3.11"
    },
    "dependencies": {
        "compromise": "^13.11.4",
        "compromise-numbers": "^1.4.0",
        "efrt": "2.7.0",
        "suffix-thumb": "5.0.2",
        "grad-school": "0.0.5"
    },
    "devDependencies": {}
}
