{
    "extends": "../../tsconfig.build.json",
    "compilerOptions": {
        "outDir": "./dist/",
        "rootDir": "./src"
    },
    "include": [
        "./src/**/*.ts",
        "./src/**/*.tsx",
        "./src/**/*.js",
        "./src/**/*.jsx"
