{
    "name": "@doenet/parser",
    "type": "module",
    "description": "Parse DoenetML",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": true,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./pretty-printer": {
            "import": "./dist/pretty-printer.js",
            "types": "./dist/pretty-printer/index.d.ts"
        },
        "./v06": {
            "import": "./dist/index-v06.js",
            "types": "./dist/index-v06.d.ts"
        },
        "./*": {
            "import": "./dist/*.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "watch": "vite build --watch",
        "build": "wireit",
        "test": "vitest",
        "compile_grammar": "npx lezer-generator --output src/generated-assets/lezer-doenet.ts src/doenet.grammar"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "src/**/*.peggy",
                "tsconfig.json",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../static-assets:build"
            ]
        }
    },
    "dependencies": {},
    "devDependencies": {}
}
