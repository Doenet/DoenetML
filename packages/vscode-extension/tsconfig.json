{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2020",
        "lib": ["es2020", "WebWorker"],
        "outDir": "out",
        "rootDir": "src",
        "sourceMap": true,
        "esModuleInterop": true,
        "paths": {
            "@doenet/*": ["../../packages/*/dist"]
        },
        "skipLibCheck": true,
        "jsx": "react-jsx"
    },
    "include": ["src"],
    "exclude": ["node_modules", ".vscode-test", "build"]
}
