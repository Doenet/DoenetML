{
	"editor.insertSpaces": false,
	"typescript.tsc.autoDetect": "off",
	"typescript.preferences.quoteStyle": "single",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"cSpell.ignoreWords": [
		"Dast",
		"clientrc",
		"documentroot",
		"doublequoted",
		"entitytype",
		"internalsubset",
		"localname",
		"singlequoted",
		"taglib"
	]
}