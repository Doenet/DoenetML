{
    "name": "@doenet/doenetml-worker",
    "type": "module",
    "description": "Repo for compiled WebWorker code for DoenetML",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": false,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/CoreWorker.js",
            "require": "./dist/CoreWorker.js"
        },
        "./*": {
            "import": "./dist/*",
            "require": "./dist/*"
        }
    },
    "scripts": {
        "watch": "vite build --watch",
        "test": "echo \"No tests \"",
        "build": "wireit"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "src/**/*.js",
                "tsconfig.json",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../utils:build",
                "../parser:build",
                "../codemirror:build"
            ]
        }
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "styled-components": "^5.3.11"
    },
    "dependencies": {
        "compromise": "^13.11.4",
        "compromise-numbers": "^1.4.0"
    },
    "devDependencies": {}
}
