{
    "name": "@doenet/test-viewer",
    "type": "module",
    "description": "Sample app showing how to use DoenetML",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": true,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "wireit",
        "test": "echo \"No tests \"",
        "preview": "vite preview"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "tsconfig.json"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../doenetml-prototype:build",
                "../ui-components:build"
            ]
        }
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^5.3.11"
    },
    "devDependencies": {
        "vite": "^4.5.0"
    }
}
