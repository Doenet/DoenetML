{
    "name": "@doenet/v06-to-v07",
    "type": "module",
    "description": "Convert DoenetML v0.6 syntax to v0.7 syntax",
    "version": "0.7.0-beta4",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": true,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./*": {
            "import": "./dist/*.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "watch": "vite build --watch",
        "build": "wireit",
        "publish": "npm run build && cd dist && npm publish",
        "test": "vitest"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "tsconfig.json",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../parser:build",
                "../doenetml-worker:build"
            ]
        }
    },
    "dependencies": {},
    "devDependencies": {}
}
