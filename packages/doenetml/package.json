{
    "name": "@doenet/doenetml",
    "type": "module",
    "description": "Semantic markup for building interactive web activities",
    "version": "0.7.0-beta10",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Doenet/DoenetML.git"
    },
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./*": {
            "import": "./dist/*",
            "require": "./dist/*"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "wireit",
        "preview": "vite preview",
        "test": "echo \"No tests \"",
        "publish": "npm run build && cd dist && npm publish"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "src/**/*.tsx",
                "src/**/*.js",
                "src/**/*.jsx",
                "src/**/*.css",
                "tsconfig.json",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.css",
                "dist/**/*.json",
                "dist/fonts/**/*"
            ],
            "dependencies": [
                "../doenetml-worker:build",
                "../codemirror:build",
                "../ui-components:build",
                "../utils:build",
                "../virtual-keyboard:build",
                "../parser:build"
            ]
        }
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "styled-components": "^5.3.11"
    },
    "dependencies": {},
    "prettier": {
        "tabWidth": 4
    }
}
