{
    "name": "@doenet/ui-components",
    "type": "module",
    "description": "Buttons for the UI",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": false,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./style.css": {
            "import": "./dist/style.css"
        }
    },
    "scripts": {
        "dev": "vite",
        "watch": "vite build --watch",
        "test": "echo \"No tests \"",
        "build": "wireit"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "tsconfig.json",
                "vite.config.ts",
                "src/**/*.js",
                "src/**/*.tsx",
                "src/**/*.jsx",
                "src/**/*.css",
                "tsconfig.json"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.d.ts",
                "dist/**/*.css",
                "dist/**/*.json"
            ]
        }
    },
    "dependencies": {},
    "devDependencies": {}
}
