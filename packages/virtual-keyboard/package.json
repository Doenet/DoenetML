{
    "name": "@doenet/virtual-keyboard",
    "type": "module",
    "description": "VirtualKeyboard component for DoenetML",
    "version": "*",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://github.com/Doenet/DoenetML#readme",
    "private": true,
    "repository": "github:Doenet/DoenetML",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./math-input": {
            "import": "./dist/math-input.js"
        },
        "./*": {
            "import": "./dist/*"
        }
    },
    "scripts": {
        "dev": "vite",
        "watch": "vite build --watch",
        "test": "echo \"No tests \"",
        "build": "wireit"
    },
    "wireit": {
        "build": {
            "command": "vite build",
            "files": [
                "src/**/*.ts",
                "src/**/*.tsx",
                "src/**/*.css",
                "tsconfig.json",
                "tailwind.config.js",
                "vite.config.ts"
            ],
            "output": [
                "dist/**/*.js",
                "dist/**/*.css",
                "dist/**/*.d.ts",
                "dist/**/*.json"
            ],
            "dependencies": [
                "../utils:build"
            ]
        }
    },
    "dependencies": {},
    "devDependencies": {}
}
